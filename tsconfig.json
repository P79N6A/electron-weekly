{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    // "allowJs": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "Interfaces": ["src/interfaces/index"],
      "Interfaces/*": ["src/interfaces/*"],
      "Components/*": ["src/components/*"],
      "Aladdin": ["src/aladdin/index"],
      "Aladdin/*": ["src/aladdin/*"],
      "Images/*": ["./src/assets/images/*"],
      "Styles/*": ["src/assets/styles/*"],
      "Utils": ["src/assets/utils/index"],
      "Utils/*": ["src/assets/utils/*"],
      "@/*": ["src/*"],
      "*": [
        "node_modules/*",
        "src/typings/*"
      ]
    },
    "declaration": true,
    "noEmit": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
